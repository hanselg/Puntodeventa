  object qrPacientes: TADOQuery
    Connection = DM.DataBase
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT c.*,g.Descripcion as Tipo FROM PtCliente c Inner Join PtG' +
        'rupoCliente g On'
      'c.GrupoCliente=g.GrupoCliente'
      'WHERE Origen = 0'
      'AND c.DataAreaId = '#39'ldr'#39' AND g.DataAreaId = '#39'ldr'#39' ')
    Left = 272
    Top = 407
