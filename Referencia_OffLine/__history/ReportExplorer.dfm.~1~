  object qrFolders: TABSQuery
    CurrentVersion = '5.04 '
    DatabaseName = 'PymeCoop'
    InMemory = False
    ReadOnly = True
    AfterOpen = qrFoldersAfterOpen
    AfterScroll = qrFoldersAfterScroll
    RequestLive = True
    SQL.Strings = (
      
        'select ModuleID, FolderID As ID, Name, ParentID, CASE WHEN Paren' +
        'tID IS Null THEN 3 ELSE 1 END As ItemType, FolderId As RecId'
      '  from SysRbFolder'
      'Where ModuleId = :pModulo'
      ''
      'UNION'
      ''
      
        'Select f.ModuleId, 999999+ItemID as ID, i.Name, i.FolderId as Pa' +
        'rentId, 2 As ItemType, ItemId As RecId'
      '  from SysRbItem i'
      ' Join SysRbFolder f On '
      '      i.FolderId = f.FolderID'
      'Where f.ModuleId = :pModulo')
    Left = 108
    Top = 104
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'pModulo'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'pModulo'
        ParamType = ptUnknown
      end>
    object qrFoldersModuleID: TSmallintField
      FieldName = 'ModuleID'
    end
    object qrFoldersName: TStringField
      FieldName = 'Name'
      Size = 60
    end
    object qrFoldersParentID: TIntegerField
      FieldName = 'ParentID'
    end
    object qrFoldersItemType: TIntegerField
      FieldName = 'ItemType'
    end
    object qrFoldersID: TIntegerField
      FieldName = 'ID'
    end
    object qrFoldersRecId: TIntegerField
      FieldName = 'RecId'
    end
  end
